{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.2.17"
    }
  },
  "crons": [
    {
      "path": "/api/cron/master-scheduler",
      "schedule": "0 2 * * *"
    }
  ]
}