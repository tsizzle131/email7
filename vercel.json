{
  "version": 2,
  "buildCommand": "cd frontend && npm ci && npm run build",
  "outputDirectory": "frontend/out",
  "installCommand": "npm ci && cd frontend && npm ci",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.2.17"
    }
  },
  "crons": [
    {
      "path": "/api/cron/follow-ups",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/process-campaigns",
      "schedule": "0 2 * * *"
    }
  ]
}