{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": ".vercel/output",
    "rootDir": "api"
  },
  "include": ["api/**/*.ts", "lib/**/*.ts"],
  "exclude": ["node_modules", "frontend"]
} 